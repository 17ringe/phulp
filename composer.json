{
    "name": "reisraff/phulp",
    "description": "The task manager for php",
    "keywords": ["phulp", "php"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rafael Reis",
            "email": "reisraff@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Phulp\\" : "src",
            "Test\\" : "test"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/finder": "^3.0"
    },
    "require-dev" : {
        "phing/phing": "2.*",
        "phpunit/phpunit": "4.5.*",
        "squizlabs/php_codesniffer": "2.0.*@dev"
    },
    "config" : {
        "bin-dir" : "bin/"
    },
    "bin": [
        "bin/phulp"
    ]
}
